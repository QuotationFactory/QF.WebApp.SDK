{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-console": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "interface-over-type-literal": false
    }
}